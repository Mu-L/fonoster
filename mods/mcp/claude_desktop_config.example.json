{
  "mcpServers": {
    "fonoster": {
      "command": "npx",
      "args": [
        "-y",
        "@fonoster/mcp"
      ],
      "env": {
        "WORKSPACE_ACCESS_KEY_ID": "your-workspace-access-key-id",
        "APIKEY_ACCESS_KEY_ID": "your-apikey-access-key-id",
        "APIKEY_ACCESS_KEY_SECRET": "your-apikey-access-key-secret"
      }
    }
  }
}